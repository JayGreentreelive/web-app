{
  "name": "christ-fellowship-react-dom",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@apollo/react-testing": "^3.1.1",
    "@babel/cli": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@christfellowshipchurch/web-fonts": "^1.0.0",
    "@christfellowshipchurch/web-ui-kit": "^0.0.36",
    "@fortawesome/fontawesome-pro-light": "^5.0.13",
    "@fortawesome/fontawesome-pro-regular": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/pro-light-svg-icons": "^5.11.2",
    "@fortawesome/pro-regular-svg-icons": "^5.11.2",
    "@fortawesome/pro-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@olavoparno/jest-badges-readme": "^1.3.6",
    "@testing-library/react": "^9.2.0",
    "apollo-boost": "^0.3.1",
    "apollo-link-context": "^1.0.19",
    "awesome-phonenumber": "^2.18.1",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "formik": "^1.5.8",
    "graphql": "^14.1.1",
    "html-to-react": "^1.3.4",
    "jest-coverage-badges": "^1.1.2",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.11.0",
    "popper.js": "^1.14.7",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.3",
    "react": "^16.8.6",
    "react-apollo": "^3.1.1",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.8.6",
    "react-facebook-pixel": "^0.1.2",
    "react-ga": "^2.5.7",
    "react-image": "^2.0.0",
    "react-meta-tags": "^0.7.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.1.1",
    "react-share": "^3.0.1",
    "react-visibility-sensor": "^5.1.1",
    "reactstrap": "^7.1.0",
    "waait": "^1.0.5",
    "yup": "^0.27.0",
    "zipcodes": "^8.0.0"
  },
  "scripts": {
    "start": "yarn build-css && react-scripts start",
    "build": "yarn minify-css && react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "fix-react": "(cd node_modules/react && yarn link) && (cd ./node_modules/react-dom && yarn link react)",
    "prestart": "node ./scripts/get-introspection-data.js",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "make-badges": "node_modules/.bin/jest-badges-readme",
    "make-readme": "npm run test:coverage && npm run make-badges",
    "build-css": "node-sass --include-path scss ./src/styles/scss/styles.scss ./src/styles/css/styles.css",
    "minify-css": "node-sass --include-path scss ./src/styles/scss/styles.scss ./src/styles/css/styles.css --output-style compressed"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.8.0",
    "react-test-renderer": "^16.8.6"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ]
  }
}
